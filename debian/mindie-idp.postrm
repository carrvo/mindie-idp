#!/bin/sh
set -e

#DEBHELPER#

if [ "$1" = "purge" ] ; then
	if true; then
		if [ -e /usr/share/apache2/apache2-maintscript-helper ] ; then
			. /usr/share/apache2/apache2-maintscript-helper

				apache2_invoke dismod selfauth  || exit 1
				apache2_invoke dismod mintoken  || exit 1
				apache2_invoke dismod sitewide-metadata  || exit 1
		fi
	fi
fi
